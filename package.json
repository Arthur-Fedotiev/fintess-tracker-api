{
    "name": "fintess-tracker-api",
    "version": "1.0.0",
    "description": "FitnessTracker backend API",
    "main": "server.js",
    "scripts": {
        "build": "rimraf dist && tsc",
        "preserve": "npm run build",
        "serve": "concurrently \"tsc --watch\" \"nodemon -q dist/server.js\"",
        "lint": "eslint src/**/*.ts",
        "format": "eslint src/**/*.ts --fix",
        "format:check": "prettier --check .",
        "prestart": "npm run build",
        "start": "cross-env NODE_ENV=production node dist/server.js",
        "format:write": "prettier --write .",
        "lint:check": "eslint .",
        "lint:fix": "eslint --fix ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Arthur-Fedotiev/fintess-tracker-api.git"
    },
    "keywords": [
        "fitness-tracker"
    ],
    "author": "Artur Fedotiev",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/Arthur-Fedotiev/fintess-tracker-api/issues"
    },
    "homepage": "https://github.com/Arthur-Fedotiev/fintess-tracker-api#readme",
    "dependencies": {
        "@typescript-eslint/eslint-plugin": "^5.18.0",
        "@typescript-eslint/parser": "^5.18.0",
        "body-parser": "^1.20.0",
        "concurrently": "^7.1.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.0",
        "express": "^4.17.3",
        "helmet": "^5.0.2",
        "rimraf": "^3.0.2",
        "typescript": "^4.6.3"
    },
    "devDependencies": {
        "@types/body-parser": "^1.19.2",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.23",
        "eslint": "^8.12.0",
        "eslint-config-google": "^0.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.25.4",
        "nodemon": "^2.0.15",
        "prettier": "^2.6.2",
        "ts-node": "^10.7.0"
    }
}
